// check krna h agar koi inpit string YES ke equal h ya nhi
#include<bits/stdc++.h>
using namespace std;
Â 
int main(){
    int t;cin>>t;
    
    while(t--){
    string s;
    cin>>s; 
    
    for(int i=0; i<s.length(); i++){
        s[i] = toupper(s[i]);
    }
    if(s=="YES") cout<<"YES"<<endl;
    else cout<<"NO"<<endl;
    }
}