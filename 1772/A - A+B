// #define ll long long int
#include<bits/stdc++.h>
using namespace std;
Â 
int main(){
   
   int t;
   cin>>t;
   
   while(t--){
        string s; cin>>s;
   int n=s.length();
   // since 0-9 digits will be pretty easy, imma write code which whill be //correct no matter the number of digits in a number
      string s1="";
      int addIndex=-1;
      
      for(int i=0; i<n; i++){
          if(s[i]=='+'){
              addIndex=i;
              break;
          }
           s1+= s[i];
      }
      string s2="";
      for(int i=addIndex+1; i<n; i++){
          s2+= s[i];
         
      }
      //cout<<s1<<"  "<<s2;
      cout<<stoi(s1)+stoi(s2)<<endl;
   }
   
}